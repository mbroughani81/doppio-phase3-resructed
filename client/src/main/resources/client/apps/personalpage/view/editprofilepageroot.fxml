<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.apps.personalpage.view.EditProfilePageRootController">
   <children>
      <Label text="Edit Profile Panel">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="newprofileButton" mnemonicParsing="false" onAction="#newprofileButtonClicked" text="New profile" />
      <Button fx:id="changenameButton" mnemonicParsing="false" onAction="#changenameButtonClicked" text="Change name" />
      <Button fx:id="changebirthdayButton" layoutX="20.0" layoutY="70.0" mnemonicParsing="false" onAction="#changebirthdayClicked" text="Change birthday" />
      <Button fx:id="changeemailButton" layoutX="20.0" layoutY="114.0" mnemonicParsing="false" onAction="#changeemailButtonClicked" text="Change email" />
      <Button fx:id="changephonenumberButton" layoutX="20.0" layoutY="70.0" mnemonicParsing="false" onAction="#changephonenumberButtonClicked" text="Change phonenumber" />
      <Button fx:id="changebioButton" layoutX="20.0" layoutY="114.0" mnemonicParsing="false" onAction="#changebioButtonClicked" text="Change bio" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
