<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.apps.personalpage.view.PersonalPageRootController">
    <children>
        <Label text="Personal Page">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
      <Button fx:id="newTweetButton" mnemonicParsing="false" onAction="#newTweetButtonClicked" text="New tweet" />
      <Button fx:id="showTweetButton" mnemonicParsing="false" onAction="#showTweetButtonClicked" text="Show tweets" />
      <Button fx:id="editprofileButton" mnemonicParsing="false" onAction="#editprofileButtonClicked" text="Edit profile" />
      <Button fx:id="showlistsButton" layoutX="10.0" layoutY="138.0" mnemonicParsing="false" onAction="#showlistsButtonClicked" text="Show lists" />
      <Button fx:id="infoButton" layoutX="10.0" layoutY="162.0" mnemonicParsing="false" onAction="#infoButtonClicked" text="Info" />
      <Button fx:id="notificationsButton" layoutX="10.0" layoutY="186.0" mnemonicParsing="false" onAction="#notificationsButtonClicked" text="Notifications" />
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
