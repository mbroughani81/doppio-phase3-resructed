<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="profileLabel" alignment="BOTTOM_LEFT" onMouseClicked="#profileLabelClicked" text="&lt;Profile&gt;" />
        <Label fx:id="tweetTextLabel" alignment="CENTER" contentDisplay="BOTTOM" graphicTextGap="10.0" onMouseClicked="#tweetTextLabelClicked" prefWidth="500.0" style="-fx-background-color: #deff70; -fx-background-radius: 10;" text="KosrqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqKosrqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqKosrqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqKosrqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqq" wrapText="true">
            <font>
                <Font size="14.0" />
            </font>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
         <graphic>
            <VBox alignment="TOP_CENTER">
               <children>
                  <Label fx:id="tweetimagelabel" contentDisplay="GRAPHIC_ONLY" text="Label" />
                  <HBox>
                     <children>
                        <Button fx:id="likeButton" mnemonicParsing="false" onAction="#likeButtonClicked" text="Like" />
                        <Region prefWidth="100.0" HBox.hgrow="ALWAYS" />
                        <Button fx:id="retweetButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#retweetButtonClicked" text="Retweet" />
                        <Region prefWidth="100.0" HBox.hgrow="ALWAYS" />
                        <Button fx:id="commentButton" layoutX="72.0" layoutY="10.0" mnemonicParsing="false" onAction="#commentButtonClicked" text="Comment" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </graphic>
        </Label>
    </children>
</HBox>
