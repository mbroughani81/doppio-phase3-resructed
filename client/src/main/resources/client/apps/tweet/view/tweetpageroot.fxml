<VBox spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.apps.tweet.view.TweetPageRootController">
   <children>
      <Label maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0" text="Tweet page Panel" VBox.vgrow="ALWAYS">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ScrollPane>
         <content>
            <VBox fx:id="mainTweetHolder" spacing="10.0">
               <children>
                  <Label layoutX="10.0" layoutY="901.0" text="Loading..." wrapText="true">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </content>
      </ScrollPane>
      <ScrollPane fx:id="scrollPane" prefHeight="216.0" prefWidth="702.0" vvalue="1.0" VBox.vgrow="ALWAYS">
         <content>
            <VBox fx:id="tweetHolder" spacing="10.0">
               <children>
                  <Label layoutX="10.0" layoutY="901.0" text="Loading..." wrapText="true">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
